/**
 * Implementation of hook_ctools_plugin_directory().
 */
function panels_three_four_layout_ctools_plugin_directory($module, $plugin) {
  if ($module == 'panels' && $plugin == 'layouts') {
    return "plugins/layouts";
  }
}